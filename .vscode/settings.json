{
    "eslint.validate": [
        "javascript",
        "javascriptvue",
        "vue"
    ],
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },

    "editor.fontFamily": "Fira Code Medium, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13
}